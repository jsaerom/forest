<h2>View Animals</h2>
#if (!($animals.isEmpty()))
  <p><a href="/animals/new">Add new animal</a></p>
  #foreach ($animal in $animals)
  <ul>
    <li><a href="/animals/$animal.getId()">$animal.getName()</a></li>
  </ul>
  #end
#else
  <p>There are currently 0 animal. <a href="/animals/new">Add an animal</a></p>
#end
